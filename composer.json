{
    "require": {
        "flightphp/core": "^3.6",
        "amplie-solucoes/ezfile": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "start": [
            "test -d vendor || composer install",
            "start /B http://127.0.0.1:8001",
